.uni-common-mt {
  padding: 10px 5px;
  background: white;
  margin: 0 15px;
  width: calc(100% - 30px);
  position: relative;
  z-index: 99;
  margin-top: -20px;
  border-radius: 5px;
}
.uni-common-mt .sec-header {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
    font-size: 14px;
    color: #303133;
    line-height: 20px;
    /* margin-left: 15px; */
    padding: 10px 0;
    border-bottom: 1px solid #eee;
    margin: 0 10px 5px;
}
.uni-common-mt .sec-header .yticon {
      font-size: 22px;
      color: #5eba8f;
      margin-right: 24rpx;
      line-height: 20px;
}
.uni-common-mt .sec-header .yticon image {
        width: 30rpx;
        height: 30rpx;
}
.uni-common-mt .sec-more {
    position: absolute;
    -webkit-align-self: center;
    -ms-flex-item-align: center;
    align-self: center;
    font-size: 15px;
    color: #606266;
    margin-left: 10px;
    right: 0px;
}
/* 头部 轮播图 */
.carousel-section {
  position: relative;
}
.carousel-section .titleNview-placing {
    height: var(--status-bar-height);
    padding-top: 44px;
    -webkit-box-sizing: content-box;
            box-sizing: content-box;
}
.carousel-section .titleNview-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 426rpx;
    -webkit-transition: .4s;
    -o-transition: .4s;
    transition: .4s;
}
.carousel {
  width: 100%;
  height: 350rpx;
}
.carousel .carousel-item {
    width: 100%;
    height: 100%;
    padding: 0;
    overflow: hidden;
}
.carousel image {
    width: 100%;
    height: 100%;
}
.swiper-dots {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  position: absolute;
  left: 60rpx;
  bottom: 15rpx;
  width: 72rpx;
  height: 36rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.swiper-dots .num {
    width: 36rpx;
    height: 36rpx;
    border-radius: 50px;
    font-size: 24rpx;
    color: #fff;
    text-align: center;
    line-height: 36rpx;
}
.swiper-dots .sign {
    position: absolute;
    top: 0;
    left: 50%;
    line-height: 36rpx;
    font-size: 12rpx;
    color: #fff;
    -webkit-transform: translateX(-50%);
        -ms-transform: translateX(-50%);
            transform: translateX(-50%);
}
.uni-page-head {
  display: none;
}

