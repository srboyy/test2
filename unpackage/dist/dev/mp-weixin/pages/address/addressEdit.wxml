<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="联系人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="联系人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">身份证</text><input class="input" type="text" placeholder="联系人身份证号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','idCard','$event',[]],['addressData']]]]]}}" value="{{addressData.idCard}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><block wx:if="{{addressData.default=='001'}}"><switch checked color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></block><block wx:else><switch color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></block></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>